##
# Log
##
error_log  /var/log/nginx/error.log;
access_log /var/log/nginx/access.log;

##
# Main
##
client_max_body_size 20M;

##
# Security
##
server_tokens off;

##
# Gzip
##
gzip on;
gzip_vary on;
gzip_proxied any;
gzip_types text/html text/xml text/css application/javascript application/json application/octet-stream;
